    "scripts": {
        "start:prod:asp": "echo Starting the development server&&webpack serve --port %PORT% --config webpack/dev.config.js",
        "start:prod:sample": "set USE_SAMPLE_DATA=true&&set USE_CONSOLE_LOGGING=true&&webpack serve --config webpack/prod.config.js",
        "start:dev:sample": "set USE_SAMPLE_DATA=true&&set USE_CONSOLE_LOGGING=true&&webpack serve --config webpack/dev.config.js --hot",
        "test": "jest --maxWorkers=1",
        "test:update-snapshots": "jest --maxWorkers=1 --updateSnapshot",
        "test:coverage": "jest --maxWorkers=1 --coverage",
        "prettier:write": "prettier --write .",
        "prettier:check": "prettier --check .",
        "lint": "eslint ./src",
        "lint:fix": "eslint --fix ./src",
        "profile": "webpack --config webpack/prod.config.js --profile --json > dist/profile.json && webpack-bundle-analyzer dist/profile.json"
    },
    "dependencies": {
